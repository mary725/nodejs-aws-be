{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "deploy": "sls deploy",
    "swagger:generate": "serverless openapi generate",
    "deploy:swagger": "npm run deploy && npm run swagger:generate",
    "deploy:getProductsList": "serverless deploy function -f getProductsList",
    "deploy:getProductById": "serverless deploy function -f getProductById",
    "deploy:createProduct": "serverless deploy function -f createProduct",
    "deploy:product-service": "npm run deploy:getProductById && npm run deploy:getProductsList && npm run deploy:createProduct",
    "invoke:getProductsList": "serverless local --function getProductsList",
    "invoke:getProductById": "serverless local --function getProductById --path=src/functions/getProductById/mock.json",
    "invoke:createProduct": "serverless local --function createProduct --path=src/functions/createProduct/mock.json",
    "invoke:locale:getProductsList": "serverless invoke local --function getProductsList",
    "invoke:locale:getProductById": "serverless invoke local --function getProductById --path=src/functions/getProductById/mock.json",
    "invoke:locale:createProduct": "serverless invoke local --function createProduct --path=src/functions/createProduct/mock.json",
    "start:offline": "sls offline",
    "test": "jest"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "dependencies": {
    "@middy/core": "^1.5.2",
    "@middy/http-cors": "^1.5.2",
    "@middy/http-json-body-parser": "^1.5.2",
    "@middy/http-urlencode-path-parser": "^1.5.2",
    "@middy/input-output-logger": "^1.5.2",
    "ajv": "^8.3.0",
    "axios": "^0.21.1",
    "pg": "^8.6.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@conqa/serverless-openapi-documentation": "^1.1.0",
    "@serverless/typescript": "^2.23.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.0.0",
    "@types/pg": "^7.14.11",
    "jest": "^26.6.3",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^2.23.0",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-offline": "^7.0.0",
    "serverless-webpack": "^5.3.5",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.20.2",
    "webpack-node-externals": "^2.5.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
